if (solid_id)
{
    speed = 0;
    residue = instance_create(x,y,obj_projectile_residue);
    residue.origin_x = x;
    residue.origin_y = y;
    residue.type = type;
    residue.dir = dir;
    residue.sprite_index = obj_game_control.projectile[type, 3];
    residue.light_size = obj_game_control.projectile[type, 7];
    residue.light_color = obj_game_control.projectile[type, 8];
    audio_play_sound_on(residue.emitter,obj_game_control.projectile[type, 5],0,1); //hit projectile sound
    with (residue)
    alarm[0] = 2*room_speed;
    script_execute(hit_script);
    instance_destroy();
}

//With Player
player_id = instance_place(x,y,obj_player);
if (player_id &amp;&amp; player_id!=creator)
{
speed = 0;
    residue = instance_create(x,y,obj_projectile_residue);
    residue.origin_x = x;
    residue.origin_y = y;
    residue.type = type;
    residue.dir = dir;
    residue.sprite_index = obj_game_control.projectile[type, 3];
    residue.light_size = obj_game_control.projectile[type, 7];
    residue.light_color = obj_game_control.projectile[type, 8];
audio_play_sound_on(player_id.emitter,obj_game_control.projectile[type, 5],0,1); //hit arrow sound
player_id.hp-=damage;
scr_screen_shake();
script_execute(hit_script);
instance_destroy();
}

